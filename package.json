{
  "name": "ts-quickstart-boilerplate",
  "version": "DEV",
  "description": "A typescript boilerplate to setup quickly a new project",
  "authors": "111tokyo",
  "license": "ISC",
  "keywords": [
    "typescript",
    "prisma",
    "boilerplate",
    "quickstart"
  ],
  "main": "./.build/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "dev": "tsc && node .",
    "start": "node .",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "db:migrate": "drizzle-kit migrate --config=./src/db/drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config=./src/db/drizzle.config.ts",
    "db:studio": "drizzle-kit studio"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "drizzle-kit": "^0.30.6",
    "prettier": "^3.5.3",
    "tslib": "^2.8.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "zod": "^3.24.3"
  },
  "dependencies": {
    "@libsql/client": "^0.15.4",
    "@types/figlet": "^1.7.0",
    "discord-rpc.ts": "^0.2.7",
    "discord.js": "^14.18.0",
    "discord.js-selfbot-v13": "^3.6.1",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.41.0",
    "figlet": "^1.8.1"
  }
}
